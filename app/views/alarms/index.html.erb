<h1>Alarms#index</h1>

<%= link_to "Add a new alarm", new_alarm_url %>

<ul class="alarms">
  <% @alarms.each do |alarm|  %>
  <li class="<%=alarm.title%>" >
  <h4><%= alarm.value %></h4>
  <h3><%=alarm.title%></h3>
  <div class="buttons">
    <%= button_to "Upvote", alarm_upvote_url(alarm) %>
    <%= button_to "Downvote", alarm_downvote_url(alarm) %>
  </div>

  </li>

  <%end %>
</ul>
